{
    "name" : "forxer/laravel-gravatar",
    "description" : "A library providing easy gravatar integration in Laravel.",
    "keywords" : [
        "gravatar",
        "laravel"
    ],
    "type" : "library",
    "license" : "MIT",
    "authors" : [{
            "name" : "Vincent Garnier",
            "email" : "forxer@gmail.org",
            "homepage" : "http://forx.fr"
        }
    ],
    "require" : {
        "php" : ">=5.6.4",
        "illuminate/support" : "~5.4.0",
        "forxer/gravatar" : "~2.0"
    },
    "autoload" : {
        "psr-4" : {
            "forxer\\LaravelGravatar\\" : "src"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "minimum-stability" : "dev",
    "support" : {
        "issues" : "https://github.com/forxer/laravel-gravatar/issues",
        "email" : "forxer@gmail.com"
    },
    "extra": {
        "laravel": {
            "providers": [
                "forxer\\LaravelGravatar\\ServiceProvider"
            ],
            "aliases":{
                "Gravatar": "forxer\\LaravelGravatar\\ServiceProvider"
            }
        }
    }
}
